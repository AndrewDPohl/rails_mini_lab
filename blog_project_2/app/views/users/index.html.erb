<% @current_user.articles.each do |article| %>

  <div class="article_container">
    <h1 class="title"><%= article.title %></h1> <br>
    <h3 class="content"><%= article.content %></h3>
    <%= button_to "Delete", article, method: :delete %>
    <%= button_to "Show Article", article_path(article), method: :get %>
    <%= button_to "Edit", edit_article_path(article), method: :get %>
  </div><br>

<% end %>